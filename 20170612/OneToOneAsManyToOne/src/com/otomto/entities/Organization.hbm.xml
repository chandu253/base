<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.otomto.entities">
	<class name="Organization" table="ORGANIZATION">
		<id name="organizationNo" column="ORGANIZATION_NO">
			<generator class="increment" />
		</id>
		<property name="organizationName" column="ORGANIZATION_NM" />
		<property name="establishedDate" column="EST_DT" />
		<property name="description" column="DESCRIPTION" />
		<property name="businessType" column="BUSINESS_TYPE" />
	</class>
</hibernate-mapping>