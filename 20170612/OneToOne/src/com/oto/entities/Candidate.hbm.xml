<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oto.entities">
	<class name="Candidate" table="CANDIDATE">
		<id name="candidateNo" column="CANDIDATE_NO">
			<generator class="increment" />
		</id>
		<property name="candidateName" column="CANDIDATE_NM" />
		<property name="appearedDate" column="APPEARED_DT" />
		<property name="appliedFor" column="APPLIED_FOR" />
		<property name="contactNo" column="CONTACT_NO" />
		<property name="emailAddress" column="EMAIL_ADDRESS" />
	</class>
</hibernate-mapping>




